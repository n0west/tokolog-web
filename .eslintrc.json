{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-require-imports": "off",
    "react-hooks/rules-of-hooks": "warn",
    "@next/next/no-img-element": "warn"
  },
  "ignorePatterns": [
    ".next",
    "node_modules",
    "*.stories.tsx",
    ".storybook",
    "out",
    "build"
  ]
}